<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <context:component-scan base-package="com.genesys.raa.**"/>


    <bean id="bus" class="com">
        <property name="service" ref="serviceBean" />

    </beans>

    <bean id="configuration" class="com.genesys.raa.agg.Configuration"/>

    <bean id="aggFactory" class="com.genesys.raa.agg.AggFactory">
            <constructor-arg ref="configuration"/>
    </bean>

    <bean id="tenantManager" class="com" factory-bean="affFactory" factory-method="getManger" init-method="" />

        <bean id="serviceBean" />


</beans>
